<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/background"
    tools:layout_editor_absoluteY="89dp"
    tools:visibility="visible">

    <TextView
        android:id="@+id/sampleNetZoneCount4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="4dp"
        android:text="0"
        android:textColor="@color/scraphead_yellow"
        android:textSize="20sp"
        app:layout_constraintEnd_toEndOf="@id/textView4"
        app:layout_constraintHorizontal_bias="0.331"
        app:layout_constraintStart_toStartOf="@id/textView4"
        app:layout_constraintTop_toBottomOf="@id/textView4" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="183dp"
        android:layout_height="42dp"
        android:layout_marginTop="8dp"
        android:text="Minor Foul"
        android:textColor="@color/scraphead_yellow"
        android:textSize="24sp"
        app:layout_constraintEnd_toEndOf="@+id/sampleNetZoneCount2"
        app:layout_constraintHorizontal_bias="0.331"
        app:layout_constraintStart_toStartOf="@+id/sampleNetZoneCount2"
        app:layout_constraintTop_toBottomOf="@+id/sampleNetZoneCount2" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="158dp"
        android:layout_height="37dp"
        android:layout_marginStart="836dp"
        android:layout_marginTop="136dp"
        android:text="Major Foul"
        android:textColor="@color/scraphead_yellow"
        android:textSize="24sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/sampleNetAdd2"
        android:layout_width="59dp"
        android:layout_height="66dp"
        android:layout_marginStart="48dp"
        android:backgroundTint="@color/scraphead_blue"
        android:onClick="netZoneIncrease2"
        android:text="+"
        android:textColor="@color/scraphead_yellow"
        android:textSize="24sp"
        app:layout_constraintBottom_toBottomOf="@+id/sampleNetZoneCount2"
        app:layout_constraintStart_toEndOf="@+id/sampleNetZoneCount2"
        app:layout_constraintTop_toTopOf="@+id/sampleNetZoneCount2"
        app:layout_constraintVertical_bias="0.555"
        tools:onClick="netZoneIncrease2" />

    <Button
        android:id="@+id/sampleNetAdd3"
        android:layout_width="59dp"
        android:layout_height="66dp"
        android:layout_marginStart="48dp"
        android:backgroundTint="@color/scraphead_blue"
        android:onClick="netZoneIncrease3"
        android:text="+"
        android:textColor="@color/scraphead_yellow"
        android:textSize="24sp"
        app:layout_constraintBottom_toBottomOf="@+id/sampleNetZoneCount4"
        app:layout_constraintStart_toEndOf="@+id/sampleNetZoneCount4"
        app:layout_constraintTop_toTopOf="@+id/sampleNetZoneCount4"
        app:layout_constraintVertical_bias="0.611" />

    <TextView
        android:id="@+id/sampleNetZoneCount2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:text="0"
        android:textColor="@color/scraphead_yellow"
        android:textSize="20sp"
        app:layout_constraintEnd_toEndOf="@+id/textView2"
        app:layout_constraintHorizontal_bias="0.387"
        app:layout_constraintStart_toStartOf="@+id/textView2"
        app:layout_constraintTop_toBottomOf="@+id/textView2" />

    <Button
        android:id="@+id/sampleNetSubtract2"
        android:layout_width="59dp"
        android:layout_height="66dp"
        android:layout_marginEnd="52dp"
        android:backgroundTint="@color/scraphead_blue"
        android:onClick="netZoneDecrease2"
        android:text="-"
        android:textColor="@color/scraphead_yellow"
        android:textSize="34sp"
        app:layout_constraintBottom_toBottomOf="@+id/sampleNetZoneCount2"
        app:layout_constraintEnd_toStartOf="@+id/sampleNetZoneCount2"
        app:layout_constraintTop_toTopOf="@+id/sampleNetZoneCount2" />

    <Button
        android:id="@+id/sampleNetSubtract3"
        android:layout_width="59dp"
        android:layout_height="66dp"
        android:layout_marginEnd="52dp"
        android:backgroundTint="@color/scraphead_blue"
        android:onClick="netZoneDecrease3"
        android:text="-"
        android:textColor="@color/scraphead_yellow"
        android:textSize="34sp"
        app:layout_constraintBottom_toBottomOf="@+id/sampleNetZoneCount4"
        app:layout_constraintEnd_toStartOf="@+id/sampleNetZoneCount4"
        app:layout_constraintTop_toTopOf="@+id/sampleNetZoneCount4" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="180dp"
        android:layout_height="35dp"
        android:layout_marginStart="28dp"
        android:layout_marginTop="96dp"
        android:text="Driver Skill Level"
        android:textColor="@color/scraphead_yellow"
        android:textSize="24sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/backSpesamen2"
        android:layout_width="107dp"
        android:layout_height="58dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="16dp"
        android:backgroundTint="@color/scraphead_blue"
        android:onClick="backSpecimen"
        android:text="Back"
        android:textColor="@color/scraphead_yellow"
        android:textSize="20sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/spesamenNextButton2"
        android:layout_width="107dp"
        android:layout_height="58dp"
        android:layout_marginTop="16dp"
        android:layout_marginEnd="16dp"
        android:backgroundTint="@color/scraphead_blue"
        android:onClick="ToQr"
        android:text="Next"
        android:textColor="@color/scraphead_yellow"
        android:textSize="20sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <CheckBox
        android:id="@+id/matchNotesOne"
        android:layout_width="289dp"
        android:layout_height="40dp"
        android:layout_marginTop="64dp"
        android:scaleX="1.2"
        android:scaleY="1.2"
        android:background="@color/scraphead_blue"
        android:checked="false"
        android:onClick="tipped"
        android:text="Bot almost tipped over"
        android:textColor="@color/scraphead_yellow"
        android:textSize="20sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <CheckBox
        android:id="@+id/matchNotesTwo"
        android:layout_width="289dp"
        android:layout_height="40dp"
        android:layout_marginTop="16dp"
        android:background="@color/scraphead_blue"
        android:onClick="BotDroppedPieces"
        android:scaleX="1.2"
        android:scaleY="1.2"
        android:text="Bot dropped pieces"
        android:textColor="@color/scraphead_yellow"
        android:textSize="20sp"
        app:layout_constraintEnd_toEndOf="@+id/matchNotesOne"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/matchNotesOne"
        app:layout_constraintTop_toBottomOf="@+id/matchNotesOne" />

    <CheckBox
        android:id="@+id/matchNotesSix"
        android:layout_width="289dp"
        android:layout_height="40dp"
        android:layout_marginTop="16dp"
        android:background="@color/scraphead_blue"
        android:onClick="MakesAGoodAlliancePartener"
        android:scaleX="1.2"
        android:scaleY="1.2"
        android:text="Compatible Partner?"
        android:textColor="@color/scraphead_yellow"
        android:textSize="20sp"
        app:layout_constraintEnd_toEndOf="@+id/matchNotesfive"
        app:layout_constraintHorizontal_bias="0.75"
        app:layout_constraintStart_toStartOf="@+id/matchNotesfive"
        app:layout_constraintTop_toBottomOf="@+id/matchNotesfive" />

    <CheckBox
        android:id="@+id/matchNotesThree"
        android:layout_width="289dp"
        android:layout_height="40dp"
        android:layout_marginTop="16dp"
        android:background="@color/scraphead_blue"
        android:onClick="BotDied"
        android:scaleX="1.2"
        android:scaleY="1.2"
        android:text="Bot died"
        android:textColor="@color/scraphead_yellow"
        android:textSize="20sp"
        app:layout_constraintEnd_toEndOf="@+id/matchNotesTwo"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/matchNotesTwo"
        app:layout_constraintTop_toBottomOf="@+id/matchNotesTwo" />

    <CheckBox
        android:id="@+id/matchNotesFour"
        android:layout_width="289dp"
        android:layout_height="40dp"
        android:layout_marginTop="16dp"
        android:background="@color/scraphead_blue"
        android:onClick="ArmWorksSlowly"
        android:scaleX="1.2"
        android:scaleY="1.2"
        android:text="Arm works slowly"
        android:textColor="@color/scraphead_yellow"
        android:textSize="20sp"
        app:layout_constraintEnd_toEndOf="@+id/matchNotesThree"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/matchNotesThree"
        app:layout_constraintTop_toBottomOf="@+id/matchNotesThree" />

    <CheckBox
        android:id="@+id/matchNotesfive"
        android:layout_width="289dp"
        android:layout_height="40dp"
        android:layout_marginTop="16dp"
        android:background="@color/scraphead_blue"
        android:onClick="BotMovesSlow"
        android:scaleX="1.2"
        android:scaleY="1.2"
        android:text="Bot moves slow"
        android:textColor="@color/scraphead_yellow"
        android:textSize="20sp"
        app:layout_constraintEnd_toEndOf="@+id/matchNotesFour"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/matchNotesFour"
        app:layout_constraintTop_toBottomOf="@+id/matchNotesFour" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="209dp"
        android:layout_height="43dp"
        android:layout_marginStart="356dp"
        android:layout_marginTop="16dp"
        android:text="Match Notes"
        android:textColor="@color/scraphead_yellow"
        android:textSize="34sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <RadioGroup
        android:layout_width="201dp"
        android:layout_height="246dp"
        android:layout_marginTop="17dp"
        app:layout_constraintEnd_toEndOf="@+id/textView3"
        app:layout_constraintStart_toStartOf="@+id/textView3"
        app:layout_constraintTop_toBottomOf="@+id/textView3">

        <RadioButton
            android:id="@+id/radioButton"
            android:layout_width="match_parent"
            android:layout_height="59dp"
            android:background="@color/white"
            android:backgroundTint="@color/white"
            android:buttonTint="@color/scraphead_yellow"
            android:onClick="skillLvlOne"
            android:text="1 Novice"
            android:textColor="@color/scraphead_blue"
            android:textSize="20sp" />

        <RadioButton
            android:id="@+id/radioButton2"
            android:layout_width="match_parent"
            android:layout_height="59dp"
            android:background="@color/white"
            android:backgroundTint="@color/white"
            android:buttonTint="@color/scraphead_yellow"
            android:onClick="skillLvlTwo"
            android:text="2 Average"
            android:textColor="@color/scraphead_blue"
            android:textSize="20sp" />

        <RadioButton
            android:id="@+id/radioButton3"
            android:layout_width="match_parent"
            android:layout_height="59dp"
            android:background="@color/white"
            android:backgroundTint="@color/white"
            android:buttonTint="@color/scraphead_yellow"
            android:onClick="skillLvlThree"
            android:text="3 Proficient"
            android:textColor="@color/scraphead_blue"
            android:textSize="20sp" />

        <RadioButton
            android:id="@+id/radioButton4"
            android:layout_width="match_parent"
            android:layout_height="59dp"
            android:background="@color/white"
            android:backgroundTint="@color/white"
            android:buttonTint="@color/scraphead_yellow"
            android:onClick="skillLvlFour"
            android:text="4 Expert"
            android:textColor="@color/scraphead_blue"
            android:textSize="20sp" />
    </RadioGroup>

    <AutoCompleteTextView
        android:id="@+id/endCommitBox"
        android:layout_width="187dp"
        android:layout_height="59dp"
        android:layout_marginTop="16dp"
        android:background="@color/scraphead_blue"
        android:hint="Comments"
        android:textAlignment="center"
        android:textColor="@color/scraphead_yellow"
        android:textColorHint="@color/scraphead_yellow"
        android:textSize="34sp"
        app:layout_constraintEnd_toEndOf="@+id/matchNotesSix"
        app:layout_constraintHorizontal_bias="0.45"
        app:layout_constraintStart_toStartOf="@+id/matchNotesSix"
        app:layout_constraintTop_toBottomOf="@+id/matchNotesSix" />

</androidx.constraintlayout.widget.ConstraintLayout>